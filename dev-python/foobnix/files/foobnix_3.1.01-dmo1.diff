@@ -0,0 +1,21 @@
+#!/usr/bin/make -f
+
+%:
+	dh $@ --with quilt,python2
+
+override_dh_auto_build:
+	python setup.py build
+
+override_dh_auto_install:
+	python setup.py install --prefix=$(CURDIR)/debian/foobnix/usr --install-layout=deb
+
+override_dh_auto_clean:
+	python setup.py clean
+
+override_dh_clean:
+	dh_clean foobnix/__init__.pyc foobnix/version.pyc test/__init__.pyc \
+	test/all.pyc
+	rm -rf dist
+
+override_dh_builddeb:
+	dh_builddeb -- -Zbzip2
